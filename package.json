  "keywords": [
    "simple color",
    "simple color converter",
    "color converter",
    "color",
    "android color converter",
    "cmyk",
    "grayscale",
    "hex3",
    "hex4",
    "hex8",
    "html",
    "hsl",
    "hsv",
    "lab",
    "pantone",
    "ral",
    "rgb",
    "rgba",
    "rgbdecimal",
    "wavelength color",
    "xyz",
    "yuv"
  ],
  "engines": {
    "node": ">= 0.8"
  },
  "main": "simple_color_converter.js",
  "repository": "draganradu/simple-color-convertor-pantone-ral",
  "homepage": "https://draganradu.github.io/",
